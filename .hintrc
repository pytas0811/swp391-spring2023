{
  "extends": [
    "development"
  ],
  "hints": {
    "axe/text-alternatives": [
      "default",
      {
        "image-alt": "off",
        "document-title": "off"
      }
    ],
    "axe/forms": "off",
    "meta-viewport": "off",
    "axe/language": "off",
    "meta-charset-utf-8": "off",
    "no-inline-styles": "off",
    "axe/name-role-value": [
      "default",
      {
        "link-name": "off",
        "button-name": "off"
      }
    ],
    "disown-opener": "off",
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "backdrop-filter",
          "-webkit-text-size-adjust",
          "-webkit-tap-highlight-color"
        ]
      }
    ]
  }
}